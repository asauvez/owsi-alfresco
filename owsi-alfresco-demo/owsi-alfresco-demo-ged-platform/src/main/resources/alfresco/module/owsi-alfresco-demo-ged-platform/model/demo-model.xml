<?xml version="1.0" encoding="UTF-8"?>
<model name="demo:contentModel"
	xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<version>1.0</version>

	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
		<import uri="http://www.alfresco.org/model/system/1.0" prefix="sys" />
	</imports>

	<namespaces>
		<namespace uri="https://demo.ged.smile.fr/ged/model/content/1.0" prefix="demo" />
	</namespaces>

	<aspects>
		<aspect name="demo:auditInfo">
			<title>Informations utiles pour l'audit</title>
			<properties>
				<property name="demo:sizeLog">
					<title>Taille du fichier</title>
					<type>d:text</type>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>false</tokenised>
					</index>
				</property>
				<property name="demo:createdMonth">
					<title>Mois de creation</title>
					<type>d:text</type>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>false</tokenised>
					</index>
				</property>
			</properties>
		</aspect>
	</aspects>

</model>