<web-fragment xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd"
	version="3.0">

	<name>redirectSlashDSlashD</name>
	<ordering><before><others/></before></ordering>
	
	<filter>
		<filter-name>TicketProvidedWebDavFilter</filter-name>
		<filter-class>fr.openwide.alfresco.repo.webdavticketprovider.TicketProvidedWebDavFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>TicketProvidedWebDavFilter</filter-name>
		<url-pattern>/aos/*</url-pattern>
	</filter-mapping>

</web-fragment>