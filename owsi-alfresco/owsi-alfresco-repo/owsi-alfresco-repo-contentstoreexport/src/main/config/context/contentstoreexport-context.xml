<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
			
	<bean id="owsi.service.contentstoreexport" 
		class="fr.openwide.alfresco.repository.contentstoreexport.service.impl.ContentStoreExportServiceImpl">
		<property name="nodeService" ref="NodeService"/>
		<property name="contentstoreexportPaths" value="${contentstoreexport.paths}"/>
		<property name="contentstoreexportQueries" value="${contentstoreexport.queries}"/>
		<property name="searchService" ref="SearchService"/>
		<property name="contentService" ref="ContentService"/>
		<property name="namespacePrefixResolver" ref="NamespaceService"/>
	</bean>
	
	<bean id="webscript.owsi.contentstoreexport.export.get" 
		class="fr.openwide.alfresco.repository.contentstoreexport.web.script.ContentStoreExportWebScript"
		parent="webscript">
		<property name="contentStoreExportService" ref="owsi.service.contentstoreexport"/> 
	</bean>
</beans>
