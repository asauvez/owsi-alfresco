<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
			
	<bean id="owsi.service.contentstoreexport" 
		class="fr.openwide.alfresco.repo.contentstoreexport.service.impl.ContentStoreExportServiceImpl">
		<property name="contentstoreexportPaths" value="${owsi.contentstoreexport.paths}"/>
		<property name="contentstoreexportQueries" value="${owsi.contentstoreexport.queries}"/>
		<property name="contentstoreexportVersion" value="${owsi.contentstoreexport.version}"/>
		<property name="serviceRegistry" ref="ServiceRegistry"/>
		<property name="descriptorService" ref ="DescriptorService"/>
		<property name="MBeanServer" ref="alfrescoMBeanServer" />
	</bean>
	
</beans>
