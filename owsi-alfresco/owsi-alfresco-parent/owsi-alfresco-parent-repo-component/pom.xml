				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>org.springframework.social</groupId>
				<artifactId>spring-social-web</artifactId>
				<version>${owsi-alfresco.repo.spring-social-web.version}</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>asm</groupId>
				<artifactId>asm</artifactId>
				<version>${owsi-alfresco.repo.asm.version}</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>com.vaadin</groupId>
				<artifactId>vaadin</artifactId>
				<version>6.8.17</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>org.codehaus.woodstox</groupId>
				<artifactId>woodstox-core-asl</artifactId>
				<version>${owsi-alfresco.repo.woodstox-core-asl.version}</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>org.codehaus.plexus</groupId>
				<artifactId>plexus-utils</artifactId>
				<version>${owsi-alfresco.repo.plexus-utils.version}</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>org.bouncycastle</groupId>
				<artifactId>bcprov-jdk15</artifactId>
				<version>${owsi-alfresco.repo.bouncycastle.version}</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>org.bouncycastle</groupId>
				<artifactId>bcmail-jdk15</artifactId>
				<version>${owsi-alfresco.repo.bouncycastle.version}</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
	            <groupId>org.alfresco.authorization</groupId>
			   	<artifactId>alfresco-authorised-users</artifactId>
				<version>${dependency.alfresco-authorised-users.version}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>

	<build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
				<excludes>
					<exclude>**/*.ori</exclude>
				</excludes>
			</resource>
			<resource>
				<directory>src/main/config</directory>
				<targetPath>alfresco/module/${alfresco.module.name}</targetPath>
				<filtering>true</filtering>
			</resource>
		</resources>

		<pluginManagement>
			<plugins>
				<plugin>
					<!--
						Ce plugin n'est plus nécessaire. On peut maintenant (Alfresco 5) mettre directement 
						module.version=${project.version} 
					-->
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>build-helper-maven-plugin</artifactId>
					<version>${owsi-alfresco.build-helper-maven-plugin.version}</version>
					<executions>
						<execution>
							<id>regex-property</id>
							<goals>
								<goal>regex-property</goal>
							</goals>
							<configuration>
								<name>alfresco.module.version</name>
								<value>${project.version}</value>
								<regex>-SNAPSHOT</regex>
								<replacement />
								<failIfNoMatch>false</failIfNoMatch>
							</configuration>
						</execution>
					</executions>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-war-plugin</artifactId>
					<configuration>
						<failOnMissingWebXml>false</failOnMissingWebXml>
						
					</configuration>
					<dependencies>
						<dependency>
							<!-- Pour utilisation https://artifacts.alfresco.com/nexus/content/repositories/alfresco-docs/alfresco-sdk-aggregator/latest/plugins/alfresco-maven-plugin/advanced-usage.html -->
							<groupId>org.alfresco.maven.plugin</groupId>
							<artifactId>alfresco-maven-plugin</artifactId>
							<version>${owsi-alfresco.alfresco-maven-plugin.version}</version>
						</dependency>
					</dependencies>
				</plugin>
			</plugins>
		</pluginManagement>
		
		<plugins>
			<plugin>
				<groupId>org.bsc.maven</groupId>
				<artifactId>maven-processor-plugin</artifactId>
				<executions>
					<execution>
						<id>maven-processor-plugin-default</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>process</goal>
						</goals>
						<configuration>
							<processors>
								<processor>fr.openwide.alfresco.repo.wsgenerator.processor.GenerateWebScriptAnnotationProcessor</processor>
							</processors>
						</configuration>
					</execution>
				</executions>
				<dependencies>
					<dependency>
						<groupId>fr.openwide.alfresco</groupId>
						<artifactId>owsi-alfresco-repo-wsgenerator-processor</artifactId>
						<version>0.6.0-SNAPSHOT</version>
					</dependency>
				</dependencies>
			</plugin>
			<plugin>
				<groupId>fr.openwide.alfresco</groupId>
				<artifactId>owsi-alfresco-repo-migration-maven-plugin</artifactId>
				<version>0.6.0-SNAPSHOT</version>
				<executions>
					<execution>
						<id>migration-maven-plugin</id>
						<phase>process-resources</phase>
						<goals>
							<goal>migration</goal>
						</goals>
						<configuration>
							<createMissingFile>true</createMissingFile>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

	<repositories>
		<repository>
			<id>alfresco-enterprise</id>
			<name>Alfresco Enterprise Artifacts</name>
			<url>https://artifacts.alfresco.com/nexus/content/groups/private</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
	</repositories>
	<pluginRepositories>
		<pluginRepository>
			<id>nexus-owsi-alfresco</id>
			<name>OWSI Alfresco</name>
			<url>https://projects.openwide.fr/services/nexus/content/repositories/owsi-alfresco</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</pluginRepository>
		<pluginRepository>
			<id>nexus-owsi-alfresco-snapshots</id>
			<name>OWSI Alfresco Snapshots</name>
			<url>https://projects.openwide.fr/services/nexus/content/repositories/owsi-alfresco-snapshots</url>
			<releases>
				<enabled>false</enabled>
			</releases>
		</pluginRepository>
	</pluginRepositories>

</project>
