	<build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
				<excludes>
					<exclude>**/*.ori</exclude>
				</excludes>
			</resource>
			<resource>
				<directory>src/main/config</directory>
				<targetPath>alfresco/module/${alfresco.module.name}</targetPath>
				<filtering>true</filtering>
			</resource>
		</resources>

		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-war-plugin</artifactId>
					<configuration>
						<failOnMissingWebXml>false</failOnMissingWebXml>
						
					</configuration>
					<dependencies>
						<dependency>
							<!-- Pour utilisation https://artifacts.alfresco.com/nexus/content/repositories/alfresco-docs/alfresco-sdk-aggregator/latest/plugins/alfresco-maven-plugin/advanced-usage.html -->
							<groupId>org.alfresco.maven.plugin</groupId>
							<artifactId>alfresco-maven-plugin</artifactId>
							<version>${owsi-alfresco.alfresco-maven-plugin.version}</version>
						</dependency>
					</dependencies>
				</plugin>
			</plugins>
		</pluginManagement>
		
		<plugins>
			<plugin>
				<groupId>org.bsc.maven</groupId>
				<artifactId>maven-processor-plugin</artifactId>
				<executions>
					<execution>
						<id>maven-processor-plugin-default</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>process</goal>
						</goals>
						<configuration>
							<processors>
								<processor>fr.openwide.alfresco.repo.wsgenerator.processor.GenerateWebScriptAnnotationProcessor</processor>
							</processors>
						</configuration>
					</execution>
				</executions>
				<dependencies>
					<dependency>
						<groupId>fr.openwide.alfresco</groupId>
						<artifactId>owsi-alfresco-repo-wsgenerator-processor</artifactId>
						<version>0.8.0</version>
					</dependency>
				</dependencies>
			</plugin>
			<plugin>
				<groupId>fr.openwide.alfresco</groupId>
				<artifactId>owsi-alfresco-repo-migration-maven-plugin</artifactId>
				<version>0.8.0</version>
				<executions>
					<execution>
						<id>migration-maven-plugin</id>
						<phase>process-resources</phase>
						<goals>
							<goal>migration</goal>
						</goals>
						<configuration>
							<enabled>${owsi.migration.enabled}</enabled>
							<createMissingFile>${owsi.migration.createMissingFile}</createMissingFile>
							<createMissingIgnoreFile>${owsi.migration.createMissingIgnoreFile}</createMissingIgnoreFile>
							<deleteIdenticalResources>${owsi.migration.deleteIdenticalResources}</deleteIdenticalResources>
							<targetWar>alfresco-enterprise</targetWar>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

