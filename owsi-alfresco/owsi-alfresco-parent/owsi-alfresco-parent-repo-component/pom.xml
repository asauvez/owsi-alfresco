				<version>${owsi-alfresco.repo.jackson.version}</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>com.fasterxml.jackson.core</groupId>
				<artifactId>jackson-annotations</artifactId>
				<version>${owsi-alfresco.repo.jackson.version}</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>edu.ucar</groupId>
