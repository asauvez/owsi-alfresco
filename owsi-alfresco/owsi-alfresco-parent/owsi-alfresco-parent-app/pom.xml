<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>fr.openwide.alfresco</groupId>
		<artifactId>owsi-alfresco-parent</artifactId>
		<version>0.1.2-SNAPSHOT</version>
	</parent>
	<artifactId>owsi-alfresco-parent-app</artifactId>
	<packaging>pom</packaging>

	<name>OWSI Alfresco - Parent - App</name>
	<description>Définitions des dépendances pour le développement d'une application spécifique sur Alfresco</description>

	<properties>
		<owsi-alfresco.owsi-core.version>0.8.7</owsi-alfresco.owsi-core.version>
		
		<owsi-alfresco.hibernate-validator.version>5.0.1.Final</owsi-alfresco.hibernate-validator.version>
		
		<owsi-alfresco.javax-el-api.version>3.0.0</owsi-alfresco.javax-el-api.version>
		<owsi-alfresco.webjars.jquery.version>1.10.2-1</owsi-alfresco.webjars.jquery.version>
		<owsi-alfresco.webjars.jquery-form.version>3.28.0-2013.02.06-1</owsi-alfresco.webjars.jquery-form.version>
		<owsi-alfresco.webjars.jquery-placeholder.version>2.0.7</owsi-alfresco.webjars.jquery-placeholder.version>
		<owsi-alfresco.webjars.jquery-cookie.version>1.4.0</owsi-alfresco.webjars.jquery-cookie.version>
		<owsi-alfresco.webjars.bootstrap.version>3.0.0</owsi-alfresco.webjars.bootstrap.version>
		<owsi-alfresco.webjars.bootstrap-datepicker.version>1.2.0</owsi-alfresco.webjars.bootstrap-datepicker.version>
	</properties>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>fr.openwide.alfresco</groupId>
				<artifactId>owsi-alfresco-parent-bom</artifactId>
				<version>0.1.2-SNAPSHOT</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			
			<dependency>
				<groupId>fr.openwide.core.packages</groupId>
				<artifactId>owsi-core-package-core-spring-jpa-app</artifactId>
				<version>${owsi-alfresco.owsi-core.version}</version>
				<type>pom</type>
				<exclusions>
					<exclusion>
						<groupId>postgresql</groupId>
						<artifactId>postgresql</artifactId>
					</exclusion>
					<exclusion>
						<groupId>org.hibernate</groupId>
						<artifactId>hibernate-search-orm</artifactId>
					</exclusion>
					<exclusion>
						<groupId>org.apache.solr</groupId>
						<artifactId>solr-core</artifactId>
					</exclusion>
					<exclusion>
						<groupId>org.springframework</groupId>
						<artifactId>spring-aspects</artifactId>
					</exclusion>
					<exclusion>
						<groupId>org.apache.poi</groupId>
						<artifactId>poi-ooxml</artifactId>
					</exclusion>
				</exclusions>
			</dependency>
			<dependency>
				<groupId>fr.openwide.core.packages</groupId>
				<artifactId>owsi-core-package-web-spring-security</artifactId>
				<version>${owsi-alfresco.owsi-core.version}</version>
				<type>pom</type>
			</dependency>
		
			<dependency>
				<groupId>org.alfresco</groupId>
				<artifactId>alfresco-jlan-embed</artifactId>
				<version>${owsi-alfresco.alfresco.version}</version>
				<exclusions>
					<exclusion>
						<groupId>org.alfresco</groupId>
						<artifactId>alfresco-core</artifactId>
					</exclusion>
					<exclusion>
						<groupId>com.hazelcast</groupId>
						<artifactId>hazelcast</artifactId>
					</exclusion>
					<exclusion>
						<groupId>com.hazelcast</groupId>
						<artifactId>hazelcast-spring</artifactId>
					</exclusion>
				</exclusions>
			</dependency>
			
			<dependency>
				<groupId>org.hibernate</groupId>
				<artifactId>hibernate-validator</artifactId>
				<version>${owsi-alfresco.hibernate-validator.version}</version>
			</dependency>
			
			<dependency>
				<groupId>javax.el</groupId>
				<artifactId>javax.el-api</artifactId>
				<version>${owsi-alfresco.javax-el-api.version}</version>
			</dependency>
			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>jquery</artifactId>
				<version>${owsi-alfresco.webjars.jquery.version}</version>
			</dependency>
			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>jquery-form</artifactId>
				<version>${owsi-alfresco.webjars.jquery-form.version}</version>
			</dependency>
			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>jquery-placeholder</artifactId>
				<version>${owsi-alfresco.webjars.jquery-placeholder.version}</version>
			</dependency>
			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>jquery-cookie</artifactId>
				<version>${owsi-alfresco.webjars.jquery-cookie.version}</version>
			</dependency>
			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>bootstrap</artifactId>
				<version>${owsi-alfresco.webjars.bootstrap.version}</version>
			</dependency>
			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>bootstrap-datepicker</artifactId>
				<version>${owsi-alfresco.webjars.bootstrap-datepicker.version}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>

</project>