		<property name="personService" ref="PersonService" />
		<property name="transactionService" ref="TransactionService" />
		<property name="searchService" ref="SearchService" />
		<property name="nodeService" ref="NodeService" />
		<property name="authorityService" ref="AuthorityService" />
		<property name="notificationService" ref="NotificationService" />
	</bean>

	<bean id="webscript.scorpion.stats.stats.get" class="fr.gouv.defense.bartoc.alfresco.repo.web.scripts.StatsWebScript" parent="webscript.objectMapper">
		<property name="gedService" ref="GedService"/>
	</bean>

	<bean id="webscript.scorpion.acp.export.get" class="fr.gouv.defense.bartoc.alfresco.repo.web.scripts.ACPExport" parent="webscript">
		<property name="searchService" ref="SearchService"/>
		<property name="exporterService" ref="ExporterService"/>
		<property name="mimetypeService" ref="MimetypeService"/>
		<property name="permissionService" ref="PermissionService"/>
		<property name="nodeService" ref="NodeService"/>
		<property name="contentService" ref="ContentService"/>
		<property name="dictionaryService" ref="DictionaryService"/>
		<property name="lockService" ref="LockService"/>
		<property name="objectMapperProvider" ref="objectMapperProvider"/>
	</bean>

	<bean id="webscript.scorpion.acp.import.post" class="fr.gouv.defense.bartoc.alfresco.repo.web.scripts.ACPImport" parent="webscript">
		<property name="searchService" ref="SearchService"/>
		<property name="importerService" ref="ImporterService"/>
