	<bean id="objectMapperProvider" class="fr.gouv.defense.bartoc.core.mapper.ObjectMapperProvider" />

	<!-- Custom "kind" of WebScript -->
	<bean id="webscript.objectMapper" parent="webscript" abstract="true"
			class="fr.gouv.defense.bartoc.alfresco.core.AbstractJacksonJavaBackedWebScript">
		<property name="objectMapperProvider" ref="objectMapperProvider" />
	</bean>

	<bean id="webscript.scorpion.users.post" class="fr.gouv.defense.bartoc.alfresco.repo.security.authentication.UsersUploadScript" parent="webscript">
