<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/decorator">

	<decorator name="simple" page="simple.jsp">
		<pattern>/security/*</pattern>
	</decorator>

	<decorator name="standard" page="standard.jsp">
		<pattern>/*</pattern>
	</decorator>

	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/ajax/*</pattern>
		<pattern>/sonde/*</pattern>
	</excludes>

</decorators>